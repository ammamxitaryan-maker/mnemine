{
  "name": "fastmine-websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for real-time notifications",
  "type": "module",
  "main": "websocket.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/index.ts",
    "build": "tsc",
    "build:prod": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:generate:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma generate",
    "prisma:generate:prod": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:push:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma db push",
    "prisma:push:prod": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy"
  },
  "keywords": [
    "websocket",
    "notifications",
    "real-time",
    "fastmine"
  ],
  "author": "FastMine Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/ws": "^8.18.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "prisma": "^6.16.3",
    "telegraf": "^4.16.3",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}