{
  "name": "fastmine-websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for real-time notifications",
  "main": "websocket.js",
  "scripts": {
    "start": "node websocket.js",
    "dev": "nodemon websocket.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:generate:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma generate",
    "prisma:generate:prod": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:push:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma db push",
    "prisma:push:prod": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:local": "DATABASE_URL=\"file:./prisma/dev.db\" prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy"
  },
  "keywords": [
    "websocket",
    "notifications",
    "real-time",
    "fastmine"
  ],
  "author": "FastMine Team",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.14.2",
    "jsonwebtoken": "^9.0.2",
    "@prisma/client": "^6.16.3",
    "prisma": "^6.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}