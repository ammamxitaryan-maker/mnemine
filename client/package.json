{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:ultra": "vite build --mode ultra-optimized",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:check": "eslint . --ext ts,tsx --max-warnings 200",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run type-check && npm run lint:check",
    "postbuild": "echo 'Build completed successfully!'",
    "validate": "npm run lint:check && npm run type-check",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist/stats.html",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "performance:audit": "npm run build && npx lighthouse http://localhost:4173 --output=html --output-path=./performance-audit.html",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "optimize:images": "npx imagemin public/**/*.{jpg,jpeg,png} --out-dir=public/optimized",
    "precommit": "npm run lint:check && npm run type-check && npm run test"
  },
  "dependencies": {
    "@dyad-sh/react-vite-component-tagger": "^0.8.0",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@types/react-window": "^2.0.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^2.7.3",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.18.6",
    "react-router-dom": "^6.30.1",
    "react-window": "^2.2.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vite": "^6.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.18.8",
    "@types/node-telegram-bot-api": "^0.64.11",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^15.15.0",
    "i18next": "^25.5.3",
    "prisma": "^6.16.3",
    "terser": "^5.44.0",
    "typescript-eslint": "^8.45.0"
  }
}